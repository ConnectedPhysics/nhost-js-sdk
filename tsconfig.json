{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "allowJs": false,
    "declaration": true,
    "sourceMap": true,
    "outDir": "./dist",
    "lib": ["es2017", "es7", "es6", "dom"],
    "strict": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
